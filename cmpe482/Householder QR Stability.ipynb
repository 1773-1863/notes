   "source": [
    "N = 50\n",
    "\n",
    "R = triu(randn(N));\n",
    "\n",
    "[Q, X] = qr(randn(N));\n",
    "\n",
    "A = Q*R;\n",
    "\n",
    "[Q2, R2] = qr(A);\n",
    "\n",
    "A2 = Q2*R2;\n",
    "\n",
    "[Q3, R3] = qr(A2);"
   ]
  },
